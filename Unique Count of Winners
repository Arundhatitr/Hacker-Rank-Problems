import java.util.*;
public class Hello {

    public static void main(String[] args) {
        int N,i;
        Scanner sc = new Scanner (System.in);
        N=sc.nextInt();
        String[] n=new String[100];
        for(i=0;i<N+1;i++)
        {
            n[i]=sc.nextLine();
        }
        Set<String> set=new HashSet<String>();
        for(i=1;i<=N;i++)
        {
            set.add(n[i]);
        }
        System.out.println(set.size());
    }
}
