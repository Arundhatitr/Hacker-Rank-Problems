#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int V,n,a[100],i;
    scanf("%d",&V);
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        if( a[i]==V)
            printf("%d",i);
    }
    return 0;
}
