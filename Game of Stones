#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int T,n,i;
    scanf("%d",&T);
    for(i=0;i<T;i++)
    {
        scanf("%d",&n);
        if(n%7>1)
            printf("First\n");
        else
            printf("Second\n");
        
    }
    return 0;
}
